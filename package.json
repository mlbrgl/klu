{
  "name": "klu",
  "version": "2.0.0",
  "dependencies": {
    "immer": "^3.1.1",
    "js-worker-search": "1.4.0",
    "localforage": "1.7.3",
    "lodash.debounce": "4.0.8",
    "luxon": "1.12.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "./node_modules/.bin/serve build",
    "lint": "./node_modules/.bin/eslint src --fix",
    "e2e": "cypress open",
    "ci": "npm run test && npm run build && npm run start & wait-on http://localhost:3000 && cypress run --record"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "eslint-plugin-jest"
    ],
    "rules": {
      "no-underscore-dangle": "off",
      "max-len": "off"
    },
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "jest/globals": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^3.3.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "serve": "^11.0.0",
    "wait-on": "^3.2.0"
  }
}
